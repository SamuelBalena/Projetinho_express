            <div class="card">
                <div class="card-body">
                    <h3>><%= perguntas.titulo %></</h3>
                </div>
                <div class="card-footer">
                    <button class="btn btn-primary">Responder</button>
                </div>
            </div>
        <% }) %>
    </div>
</body>
